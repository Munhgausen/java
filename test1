public class Box {
    double width;
    double height;
    double depth;

    //рассчитать объем
    double volume(){
        return width*height*depth;
    }

    void setDim(double w,double h,double d){
        width=w;
        height=h;
        depth=d;
    }
}

class BoxDemo4 {
    public  static  void  main(String args[]){
        Box mybox1=new Box();
        Box mybox2=new Box();
        double vol;

        mybox1.setDim(10,20,15);
        mybox2.setDim(3,6,9);

        vol=mybox1.volume();
        System.out.println("Объем 1 ящика= "+ vol);

        vol=mybox2.volume();
        System.out.println("Объем 2 ящика = "+ vol);




    }
}
